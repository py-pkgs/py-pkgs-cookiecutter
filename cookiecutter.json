{
  "author_name": "Monty Python",
  "package_name": "mypkg",
  "repository": "https://github.com/placeholder",
  "__package_slug": "{{ cookiecutter.package_name|lower|replace(' ', '_')|replace('-', '_') }}",
  "__package_env_slug": "{{ cookiecutter.__package_slug|upper }}",
  "package_short_description": "A package for doing great things!",
  "package_version": "0.1.0",
  "python_version": "3.8",
  "open_source_license": [
    "MIT",
    "Apache License 2.0",
    "GNU General Public License v3.0",
    "CC0 v1.0 Universal",
    "BSD 3-Clause",
    "Proprietary",
    "None"
  ],
  "include_github_actions": [
    "no",
    "ci",
    "ci+cd"
  ],
  "_copy_without_render": [
    ".github/*.yml",
    "docs/*.md"
  ]
}
